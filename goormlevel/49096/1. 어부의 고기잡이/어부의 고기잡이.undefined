#include <iostream>
#include <vector>
#include <algorithm>
#include <numeric>
using namespace std;
int main() {

	int n,m;
	cin>>n>>m;
	vector<int> fish(n+1);
	for(int i = 1;i<=n;i++)cin>>fish[i];

	int ans = 0;
	 int s = 1, e = 1;

    while (e <= n) 
			{
				
			long long cnt = 0;
				for(int i = s;i<=e;i++)
					{
						cnt+=fish[i];
					}
        if (cnt >= m)
				{
          s++; 
					if(cnt==m)
					ans++;
				} 
				else 
				{
          e++;
        }
    }
	cout<<ans;
	return 0;
}