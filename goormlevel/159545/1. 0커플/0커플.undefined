#include <iostream>
#include<unordered_map>
using namespace std;
int main() {
	int n;
	cin>>n;

	unordered_map<int,int> um;
	for(int i = 0;i<n;i++)
		{
			int num;
			cin>>num;
			if(um.find(num) != um.end())
			{
				um[num]--;
			}
			else
			{
				um[num]++;
			}
		}
	int ans = 0;
	for(auto i : um)
		{
			ans+=i.first;
		}
	cout<<ans;
	return 0;
}